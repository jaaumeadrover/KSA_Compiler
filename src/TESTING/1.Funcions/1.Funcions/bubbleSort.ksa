proc bubbleSort(int array A, int n,bool z){
    int aux;
    for (int i <- 0; i < n - 1; i <- i + 1) {
        for (int j <- 0; j < (n- i) - 1; j <- j + 1) {
           int c;
              if (A[j + 1] < A[j]) {
                aux <- A[j + 1];
                A[j + 1] <- A[j];
                A[j] <- aux;
              }
        }
    }
}

